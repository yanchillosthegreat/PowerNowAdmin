<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] </title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <partial name="Shared/_CssImports" />
    @*Html.ApplicationInsightsJavaScript(TelemetryConfiguration)*@
</head>
<body class="top-navigation pace-done md-skin" style="background-color: #1a1e20;">
    <partial name="Shared/_JsImports" />

    <!-- Wrapper-->
    <div class="wrapper" style="padding: 0; min-height:100%; margin-bottom: -200px;">
        <!-- Page wraper -->
        <div  class="gray-bg">
            <!--Header-->
            <partial name="Shared/_SiteNavigation" />
            <!-- Main view  -->
            <div class="container animated fadeInDown">
                <div class="row">
                <div class="col-sm-4"></div>
                <div class="col-sm-4 offset-sm-4 col-xs-12">
                    @RenderBody()
                </div>
            </div>
            </div>
            
            
        </div>
        
        <!-- End page wrapper-->
        <div class="push" style="height: 200px;"></div>
    </div>
    <!-- End wrapper-->
    <!-- Footer -->
    <partial name="Shared/_Footer" />


    @RenderSection("scripts", required: false)



</body>
</html>