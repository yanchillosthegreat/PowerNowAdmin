<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] </title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link href="https://fonts.googleapis.com/css?family=Roboto&display=swap" rel="stylesheet">
    <script src="https://api-maps.yandex.ru/2.1/?lang=ru_RU" type="text/javascript"></script>

    <partial name="Shared/_CssImports" />
    @*Html.ApplicationInsightsJavaScript(TelemetryConfiguration)*@
</head>
<body class="top-navigation pace-done md-skin" style="background-color: #1a1e20;">
    <partial name="Shared/_JsImports" />

    <!-- Wrapper-->
    <div class="wrapper" style="padding: 0; min-height:100%; margin-bottom: -200px;">
        <!-- Page wraper -->
        <div  class="gray-bg">
            <!--Header-->
            <partial name="Shared/_SiteNavigation" />
            <!-- Main view  -->
            <div class="container animated fadeInDown
                        col-lg-2 col-lg-offset-5
                        col-md-2 col-md-offset-5
                        col-sm-2 col-sm-offset-5
                        col-xs-8 col-xs-offset-2">
                <div class="row">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    <!-- End page wrapper-->
    <div class="push" style="height: 200px;"></div>
    <!-- End wrapper-->
    <!-- Footer -->
    <partial name="Shared/_Footer" />


    @RenderSection("scripts", required: false)



</body>
</html>